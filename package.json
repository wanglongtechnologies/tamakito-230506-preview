{
  "name": "tamakito",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "html"
    }
  },
  "scripts": {
    "serve": "concurrently \"postcss tailwind.css -o src/assets/css/styles.css --watch\"  \"live-server ./src\"",
    "development": "postcss tailwind.css -o src/assets/css/styles.css",
    "production": "postcss tailwind.css -o src/assets/css/styles.css",
    "start": "npm run serve"
  },
  "keywords": [],
  "author": "WANGLONG",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-selector-parser": "^6.0.12"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "live-server": "^1.2.2",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.2"
  }
}
